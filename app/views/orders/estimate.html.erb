<div class="row col-md-4 col-md-offset-4">
  <% @ups.each do |quote| %>
    <p>
    carrier: <%= quote["carrier"] %><br>
    shipping cost: <%= number_to_currency(quote["total_price"] / 100) %><br>
    service type: <%= quote["service_type"] %><br>
    delivery date: <%= quote["expected_delivery"] ? pretty_date(quote["expected_delivery"]) : "No estimate available" %>
    </p>
  <% end %>
  <% @usps.each do |quote| %>
    <p>
    carrier: <%= quote["carrier"] %> <br>
    shipping cost: <%= number_to_currency(quote["total_price"] / 100) %><br>
    service type: <%= quote["service_type"] %><br>
    delivery date: <%= quote["expected_delivery"] ? pretty_date(quote["expected_delivery"]) : "No estimate available" %>
    </p>
  <% end %>
</div>
